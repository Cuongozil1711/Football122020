<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Homecontrol</servlet-name>
        <servlet-class>control.Homecontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategoryControl</servlet-name>
        <servlet-class>control.CategoryControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Detailcontrol</servlet-name>
        <servlet-class>control.Detailcontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchControl</servlet-name>
        <servlet-class>control.SearchControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logincontrol</servlet-name>
        <servlet-class>control.Logincontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>control.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Managercontrol</servlet-name>
        <servlet-class>control.Managercontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddproductControl</servlet-name>
        <servlet-class>control.AddproductControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartControl</servlet-name>
        <servlet-class>control.CartControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddcartControl</servlet-name>
        <servlet-class>control.AddcartControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartAddControl</servlet-name>
        <servlet-class>control.CartAddControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCart</servlet-name>
        <servlet-class>control.DeleteCart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Homecontrol</servlet-name>
        <url-pattern>/Homecontrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoryControl</servlet-name>
        <url-pattern>/Category</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Detailcontrol</servlet-name>
        <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchControl</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logincontrol</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Managercontrol</servlet-name>
        <url-pattern>/manager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddproductControl</servlet-name>
        <url-pattern>/addproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartControl</servlet-name>
        <url-pattern>/addcart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddcartControl</servlet-name>
        <url-pattern>/Addcart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartAddControl</servlet-name>
        <url-pattern>/cartadd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCart</servlet-name>
        <url-pattern>/deletecart</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
